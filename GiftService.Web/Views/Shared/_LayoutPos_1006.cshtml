<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type">
    <title>Knygynai.lt</title>
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/Site.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/p1006/style.css">
    <script src="http://jqueryjs.googlecode.com/files/jquery-1.3.2.min.js" type="text/javascript"></script>
    <style type="text/css">
        #top {
            box-sizing: unset !important;
        }
        #menu {
            box-sizing: unset !important;
        }
        #search {
            box-sizing: unset !important;
        }
        .searchfield {
            box-sizing: unset !important;
        }
        #txtboxes {
            box-sizing: unset !important;
        }
    </style>
</head>
<body>
    <div style="display: none" class="christmas"></div>
    <div id="wrap">
        <div id="top">
            <div class="logo">
                <a href="http://www.knygynai.lt/"><img border="0" alt="knygynai.lt" src="http://www.knygynai.lt/view/img/logo.gif"></a>
            </div>
        </div>
        <div style="clear: both"></div>
        <div style="clear:both"></div>
        <div id="menu">
            <div class="menutop">
                <div class="menuline">
                    <div class="menulink">
                        <div class="menulink_beg">
                            <div class="menulink_back">
                                <div class="menulink_lnk">
                                    <a href="http://www.knygynai.lt/apie-projekta">Apie projektą</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="menulink">
                        <div class="menulink_beg">
                            <div class="menulink_back">
                                <div class="menulink_lnk">
                                    <a href="http://www.knygynai.lt/knygynu-paieska">Knygynų paieška</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="menulink">
                        <div class="menulink_beg">
                            <div class="menulink_back">
                                <div class="menulink_lnk">
                                    <a href="http://www.knygynai.lt/interviu">Interviu</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="secmenuline">
                    <div class="secmenuline_lnk"><a href="http://www.knygynai.lt/leidykloms">Leidykloms</a></div>
                    <div class="secmenuline_lnk"><a href="http://www.knygynai.lt/rasytojams">Rašytojams</a></div>
                    <div class="secmenuline_lnk"><a href="http://www.knygynai.lt/knygynams">Knygynams</a></div>
                    <div class="secmenuline_lnkLST"><a href="http://www.knygynai.lt/ieskomos-knygos">Ieškomos knygos</a></div>
                </div>
            </div>

        </div>
        <div id="search">
            <form method="get" action="http://www.knygynai.lt/knygos">
                <div class="searchfield">
                    <input type="text" value="Įveskite ieškomos knygos žodį arba autorių" id="" class="searchinput" name="q" onfocus="if(this.value=='Įveskite ieškomos knygos žodį arba autorių') this.value=''">
                </div>
                <div class="searchbutton"><input type="image" border="0" alt="-" src="http://www.knygynai.lt/view/img/search_button.gif"></div>
            </form>
        </div>	<div id="txtboxes">

            @RenderBody()

        </div>



        <div id="bottom">
        </div>
        <div id="footer">
            <span class="footsize">
                <a href="http://www.knygynai.lt/apie-projekta">Apie projektą</a>  |
                <a href="http://www.knygynai.lt/">Knygų paieška</a>  |
                <a href="http://www.knygynai.lt/interviu">Interviu</a>
            </span><br>
            <span class="footsize2"><a href="http://www.knygynai.lt/leidykloms">Leidykloms</a>  |  <a href="http://www.knygynai.lt/rasytojams">Rašytojams</a>  |  <a href="http://www.knygynai.lt/knygynams">Knygynams</a>  |  <a href="http://www.knygynai.lt/ieskomos-knygos">Ieškomos knygos</a></span><br>
            &copy; knygynai.lt. Visos teisės saugomos. Sprendimas: <span class="sprendimas"><a href="http://www.interateitis.lt">Interateitis</a></span>.

            <span style="margin: 0 0 0 10px;" class="sprendimas"></span>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)

</body>
</html>
